{
    "require": {
        "illuminate/database": "^5.7",
        "lefuturiste/slim3_multilanguage": "^0.8.3",
        "monolog/monolog": "^1.23",
        "slim/slim": "3.0",
        "slim/twig-view": "^2.4"
    },
    "autoload": {
        "psr-4": {
          "App\\": "src",
          "App\\Models\\": "models",
          "App\\Controllers\\": "controllers"
        }
      },
      "autoload-dev": {
        "psr-4": {
          "App\\Test\\": "tests"
        }
      },
      "config": {
        "sort-packages": true
      }
}
